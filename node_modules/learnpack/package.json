{
  "name": "learnpack",
  "description": "Seamlessly build or take interactive & auto-graded tutorials, start learning now or build a tutorial.",
  "version": "0.1.26",
  "author": "Alejandro Sanchez @alesanchezr",
  "bin": {
    "learnpack": "./bin/run"
  },
  "bugs": "https://github.com/learnpack/learnpack-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^3.1.0",
    "@oclif/plugin-plugins": "^1.8.0",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.0",
    "cli-ux": "^5.4.6",
    "cors": "^2.8.5",
    "debounce": "^1.2.0",
    "dotenv": "^8.2.0",
    "enquirer": "^2.3.6",
    "eta": "^1.2.0",
    "express": "^4.17.1",
    "front-matter": "^4.0.2",
    "fs-extra": "^9.1.0",
    "git-repo-info": "^2.1.1",
    "moment": "^2.27.0",
    "node-emoji": "^1.10.0",
    "node-fetch": "^2.6.0",
    "node-persist": "^3.1.0",
    "prompts": "^2.3.2",
    "request": "^2.88.2",
    "shelljs": "^0.8.4",
    "socket.io": "^2.3.0",
    "targz": "^1.0.1",
    "validator": "^13.1.1",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/plugin",
    "/src"
  ],
  "homepage": "https://learnpack.co",
  "keywords": [
    "learn",
    "learn to code",
    "tutorial",
    "tutorials",
    "coding tutorials",
    "education",
    "coding education"
  ],
  "license": "https://learnpack.co/terms",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "learnpack",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-warn-if-update-available"
    ],
    "permanent_plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "repository": "learnpack/learnpack-cli",
  "pre-commit": {
    "silent": true,
    "run": [
      "pre"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "pre": "node ./test/precommit/index.js",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
